<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_auto_checkout_attendance" model="ir.cron">
            <field name="name">HR: Auto Checkout for Missed Attendances</field>
            <field name="model_id" ref="model_hr_notification_cron"/>
            <field name="state">code</field>
            <field name="code">model._execute_auto_checkout()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 23:55:00')" />
        </record>
    </data>
</odoo>